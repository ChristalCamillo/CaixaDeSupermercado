<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.zup.caixadesupermercado">

    <application
        android:allowBackup="true"
        android:icon="@drawable/carrinho"
        android:label="@string/app_name"
        android:roundIcon="@drawable/carrinho"
        android:supportsRtl="true"
        android:theme="@style/Theme.CaixaDeSupermercado">
        <activity
            android:name="com.example.caixadesupermercado.informacao.InformacaoActivity"
            android:exported="false"/>
        <activity
            android:name="com.example.caixadesupermercado.home.HomeActivity"
            android:exported="true"
            android:label="@string/title_activity_home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
     </application>
    </manifest>